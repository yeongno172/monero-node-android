		"--detach --config-file /data/data/com.termux/files/home/storage/external-1/bitmonero/monerod.conf",
		"--detach --config-file /data/data/com.termux/files/home/storage/external-1/bitmonero/monerod_p2p.conf",
		"--detach --config-file /data/data/com.termux/files/home/storage/external-1/bitmonero/monerod_rpc.conf"
	],
	

	
